<p id="notice"><%= notice %></p>


<div class="col-lg-12">
  <div class="page-header">
    <h1 id="tables">Campaigns</h1>
  </div>

  <div class="bs-component">
    <table class="table table-striped table-hover ">
      <thead>
        <tr>
          <th>Campaign</th>
          <th>Date Of Follow up</th>
          <th>Contacts Reached</th>

        </tr>
      </thead>

      <tbody>
        <% @campaign_followups.each do |cf| %>
          <tr>
            <td><%= @campaign.name %></td>
            <td><%= cf.created_at.strftime("%d/%m/%Y") %></td>
            <td><%= cf.email_followups.count %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  <div id="source-button" class="btn btn-primary btn-xs" style="display: none;">&lt; &gt;</div></div><!-- /example -->
</div>
